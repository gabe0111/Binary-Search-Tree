Recursive approach

// User function Template for C++

void guide(struct Node* node, vector<int>& res){
    if(node==NULL){
        return;
    }
    res.push_back(node->data);
    guide(node->left,res);
    guide(node->right,res);
}
vector<int> preOrder(struct Node* node) {
    // Your code here
    vector<int> res;
    guide(node,res);
    return res;
}

